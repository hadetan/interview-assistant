# Copy to .env and replace placeholder values.
TRANSCRIPTION_PROVIDER=assembly
ASSEMBLYAI_API_KEY=your-assemblyai-api-key
TRANSCRIPTION_ENABLED=true
# Timeout in milliseconds before aborting a transcription request (optional)
TRANSCRIPTION_TIMEOUT_MS=20000
# MediaRecorder timeslice for audio chunks in ms
TRANSCRIPTION_CHUNK_TIMESLICE_MS=150